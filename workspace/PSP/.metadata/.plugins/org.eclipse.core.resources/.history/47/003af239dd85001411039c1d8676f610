class Productor extends Thread{
	private Cola cola;
	private String s;

	public Productor (Cola c,String s){
		cola=c;
		this.s=s;
	}
	
	public void run(){
		for (int i=0;i<5;i++){
			cola.put(i); //pone el nÃºmero
			System.out.println(i	+"->Productor:"+ n + ", produce: "+i);
 			try {
				sleep(100);
			}catch (InterruptedException e){}
		}
	}
}
